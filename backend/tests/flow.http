# Create tenant (idempotent)
POST http://localhost:4000/api/tenants
Content-Type: application/json

{
  "name": "default",
  "slug": "default"
}

###
# Create teacher
POST http://localhost:4000/api/tenants/default/teachers
Content-Type: application/json

{
  "name": "Alice Teacher",
  "email": "alice@example.com",
  "subjects": ["Maths"],
  "grade": "Grade 4"
}

###
# Upload timetable (replace teacherId and file path)
POST http://localhost:4000/api/tenants/default/teachers/{{teacherId}}/timetable/extract
Content-Type: multipart/form-data; boundary=boundary

--boundary
Content-Disposition: form-data; name="file"; filename="Teacher Timetable Example 2.pdf"
Content-Type: application/pdf

< ./examples/Teacher\ Timetable\ Example\ 2.pdf
--boundary--

###
# Get latest timetable
GET http://localhost:4000/api/tenants/default/teachers/{{teacherId}}/timetable

